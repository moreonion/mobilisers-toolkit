{
  "name": "mobilisers-toolkit",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "astro check && pnpx sv check",
    "test": "playwright test",
    "test:quick": "playwright test tests/quick-check.spec.ts",
    "test:clean-html": "playwright test tests/clean-html.spec.ts",
    "test:prepopulation": "playwright test tests/prepopulation-link.spec.ts",
    "test:share": "playwright test tests/share-link.spec.ts",
    "test:tracking": "playwright test tests/tracking-link.spec.ts",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.ts,.astro,.svelte && prettier --check .",
    "format": "prettier -w 'src/**/*.{astro,css,html,json,md,mdx,js,jsx,ts,tsx,svelte}'",
    "lint:fix": "eslint --fix . --ext .js,.ts,.astro,.svelte"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/svelte": "^7.1.0",
    "astro": "^5.9.4",
    "jstat": "^1.9.6",
    "svelte": "^5.34.1",
    "svelte-persisted-store": "^0.7.0",
    "svelte-unstyled-tags": "^0.0.15",
    "typescript": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@playwright/test": "^1.48.0",
    "@types/js-beautify": "^1.14.3",
    "@types/sanitize-html": "^2.9.5",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "js-beautify": "^1.14.11",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.34.2",
    "svelte-check": "^4.2.2",
    "typescript-eslint": "^8.20.0",
    "vitest": "^3.2.4",
    "zod": "^3.22.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}